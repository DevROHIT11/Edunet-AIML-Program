   "source": [
    "## Calculate the avg of temperature"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5296259c-b432-49b0-8818-903b95b8778a",
   "metadata": {},
   "outputs": [],
   "source": [
    "weekly_temp = [25, 27, 28, 26, 24, 30, 29]\n",
    "\n",
    "def avg(val):\n",
    "    sum = 0\n",
    "    for i in val:\n",
    "        sum += i\n",
    "    avg = sum / len(val)\n",
    "    print(f\"The average is {avg}\")\n",
    "\n",
    "avg(weekly_temp)\n",
    "      "
   ]
  }
